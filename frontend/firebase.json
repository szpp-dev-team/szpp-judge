{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "cleanUrls": true,
    "rewrites": [
      {
        "regex": "^/contests/([^/]+?)(?:/)?$",
        "destination": "/contests/[contest_slug].html"
      },
      {
        "regex": "^/contests/([^/]+?)/submissions/([^/]+?)(?:/)?$",
        "destination": "/contests/[contest_slug]/submissions/[submission_id].html"
      },
      {
        "regex": "^/contests/([^/]+?)/tasks(?:/)?$",
        "destination": "/contests/[contest_slug]/tasks.html"
      }
    ]
  }
}
